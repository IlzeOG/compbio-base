<Window x:Class="BaseLib.Wpf.DictionaryIntValuePopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:param="clr-namespace:BaseLib.Param"
        Title="" Height="400" Width="340">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <param:ParameterPanel x:Name="ParameterPanel"></param:ParameterPanel>
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="60"></ColumnDefinition>
                <ColumnDefinition Width="60*"></ColumnDefinition>
                <ColumnDefinition Width="60"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Button x:Name="CancelButton" Click="CancelButton_OnClick">Cancel</Button>
            <Button x:Name="OkButton" Click="OkButton_OnClick" Grid.Column="2">OK</Button>
        </Grid>
    </Grid>
</Window>
